<?xml version="1.0" encoding="utf-8" standalone="no"?>
<security>
	<roles_hierarchy name="user_role">
	</roles_hierarchy>
	<firewall>
		<config>
			<login>
				<source id="login" vars="fr"/>
			</login>
			<default>
				<source id="index" vars="fr"/>
			</default>
			<forbidden template="gcs/error">
				<variable type="lang" name="error" value="gc_firewallforbiddentitle"/>
				<variable type="lang" name="explanation" value="gc_firewallforbiddencontent"/>
			</forbidden>
			<csrf name="user_token" template="gcs/error" enabled="true">
				<variable type="lang" name="error" value="gc_firewallcsrftitle"/>
				<variable type="lang" name="explanation" value="gc_firewallcsrfcontent"/>
			</csrf>
			<connect name="user_logged" yes="true" no="false"/>
		</config>
		<access>
		</access>
	</firewall>
</security>
